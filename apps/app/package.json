{
  "name": "@pikatorrent/app",
  "version": "0.4.1",
  "scripts": {
    "build:android:development": "cross-env TAMAGUI_TARGET=native eas build --profile development --platform android --local",
    "build:android:preview": "cross-env TAMAGUI_TARGET=native eas build --profile preview --platform android --local",
    "build:android:production": "cross-env TAMAGUI_TARGET=native eas build --profile production --platform android --local",
    "native": "cross-env TAMAGUI_TARGET=native APP_VARIANT=development expo start --dev-client -p 8082",
    "android": "cross-env TAMAGUI_TARGET=native APP_VARIANT=development expo start --android --dev-client -p 8082",
    "ios": "cross-env TAMAGUI_TARGET=native APP_VARIANT=development expo start --ios --dev-client -p 8082",
    "web": "cross-env TAMAGUI_TARGET=web expo start --web",
    "build:web": "cross-env TAMAGUI_TARGET=web expo export --platform web",
    "lint": "eslint ."
  },
  "dependencies": {
